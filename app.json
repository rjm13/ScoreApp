{
  "expo": {
    "name": "ScoreVerse",
    "slug": "ScoreVerse",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Logo.png",
    "splash": {
      "image": "./assets/ScoreboardSplash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/LogoVerse.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.msm.ScoreBoardApp"
    },
    "web": {
      "favicon": "./assets/Logo.png"
    }
  }
}
